<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/catalogoImagen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/common_signin_btn_icon_dark" />

    <TextView
        android:id="@+id/catalogoNombre"
      	android:layout_below="@id/catalogoImagen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hola" />

    <ImageView
        android:id="@+id/catalogoSuscrito"
        android:layout_toRightOf="@id/catalogoNombre"
        android:layout_alignParentRight="true"
        android:layout_below="@id/catalogoImagen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_check" />

</RelativeLayout>